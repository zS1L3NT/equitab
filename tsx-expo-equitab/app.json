{
	"expo": {
		"name": "Equitab",
		"slug": "equitab",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"plugins": [
			[
				"expo-contacts",
				{
					"contactsPermissions": "This allows us to find your friends by their phone number"
				}
			]
		],
		"jsEngine": "hermes",
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.anonymous.equitab"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"permissions": [
				"android.permission.READ_CONTACTS",
				"android.permission.WRITE_CONTACTS"
			]
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"bundler": "metro"
		}
	}
}